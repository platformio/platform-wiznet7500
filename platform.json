{
  "name": "wiznet7500",
  "title": "WIZNet W7500",
  "description": "The IOP (Internet Offload Processor) W7500 is the one-chip solution which integrates an ARM Cortex-M0, 128KB Flash and hardwired TCP/IP core for various embedded application platform especially requiring Internet of things",
  "homepage": "http://www.wiznet.io/product-item/w7500/",
  "license": "Apache-2.0",
  "keywords": [
      "dev-platform",
      "ARM",
      "Cortex-M",
      "WIZNet",
      "W7500",
      "Ethernet",
      "IOP"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-wiznet7500.git"
  },
  "version": "3.6.2",
  "frameworks": {
    "mbed": {
      "package": "framework-mbed",
      "script": "builder/frameworks/mbed.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "owner": "platformio",
      "version": ">=1.60301.0,<1.80000.0"
    },
    "framework-mbed": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~6.51506.0"
    },
    "tool-pyocd": {
      "type": "debugger",
      "optional": true,
      "owner": "platformio",
      "version": "~0.801.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "^1.63208.0"
    }
  }
